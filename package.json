{
  "name": "discord-bot",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "tsx src",
    "format": "prettier --write \"src/**/*.ts\"",
    "dev": "tsx watch src",
    "lint": "eslint ./src --ext .js,.jsx,.ts,.tsx",
    "migrate:latest": "tsx src/database/migrate/bin",
    "gen:types": "kysely-codegen --camel-case --dialect sqlite --out-file src/database/types.ts",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daniiar-pro/discord-bot.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "module",
  "bugs": {
    "url": "https://github.com/daniiar-pro/discord-bot/issues"
  },
  "homepage": "https://github.com/daniiar-pro/discord-bot#readme",
  "description": "",
  "dependencies": {
    "@giphy/js-fetch-api": "^5.6.0",
    "axios": "^1.7.9",
    "better-sqlite3": "^11.7.2",
    "discord.js": "^14.17.3",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "http-status-codes": "^2.3.0",
    "kysely": "^0.27.5",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.18.0",
    "@tsconfig/node23": "^23.0.0",
    "@types/better-sqlite3": "^7.6.12",
    "@types/express": "^5.0.0",
    "@types/node": "^22.10.5",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^8.19.1",
    "@typescript-eslint/parser": "^8.19.1",
    "@vitest/coverage-v8": "^2.1.8",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.14.0",
    "kysely-codegen": "^0.17.0",
    "prettier": "^3.4.2",
    "supertest": "^7.0.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.19.1",
    "vitest": "^2.1.8"
  }
}
